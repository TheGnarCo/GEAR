<%= turbo_frame_tag "gnar-users" do %>
  <table>
    <thead>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">Actions</th>
    </thead>

    <tbody>
      <% users.each do |user| %>
        <%= render "gnar/users/user", user: user %>
      <% end %>
    </tbody>
  </table>
<% end %>
